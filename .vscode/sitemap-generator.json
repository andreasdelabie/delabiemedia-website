{
  "sitemap.xml": {
    "Protocol": "https",
    "DomainName": "delabiemedia.be",
    "Root": "./",
    "IncludeExt": [
      ".html",
      ".php"
    ],
    "Exclude": [
      "404.html",
      "assets/*",
      "projecten/.template/*",
      "reviews/*",
      "mailinglist/*",
      "privacy/*",
      "cookies/*",
      "cadeaubon/*",
      "algemene-voorwaarden/*"
    ],
    "TagsToInclude": [
      "priority",
      "lastmod"
    ],
    "DefaultChangeFrequency": "monthly",
    "TabCharacters": "\t",
    "bIncludeWWW": false,
    "bRemoveFileExtentions": false,
    "bUseTrailingSlash": true,
    "bAutomaticallyUpdateSitemap": true,
    "bMinimized": false
  }
}