.page{min-height:100vh;padding:200px 100px 100px;display:flex;justify-content:center;background-color:var(--white)}@media screen and (max-width:768px){.page{padding:125px 10px 50px}}.page .content{max-width:75ch;overflow-wrap:anywhere}.page .content a{color:var(--logo-magenta);text-decoration:none;overflow-wrap:anywhere}.page .content a:hover{color:var(--logo-green);text-decoration:underline}.page .content .doc-qr{margin-top:25px}.page .content .links{margin-top:25px}